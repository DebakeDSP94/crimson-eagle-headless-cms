{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JwtSettings": {
    "Issuer": "Crimson-Eagle",
    "Audience": "CE-CMS-API",
    "DurationInMinutes": 30,
    "Key": "PlaceSecurityKeyHereToBeHashedAndComparedWithEachToken"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "HotelListingDbConnectionString": "Server=CANDYROLL93;Database=CE-CMS-APIDB;Trusted_Connection=True;TrustServerCertificate=True;MultipleActiveResultSets=True;"
  },

  "Serilog": {
    "MinimalLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5027"
        }
      }
    ]
  }
